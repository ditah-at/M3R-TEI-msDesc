    <skos:ConceptScheme rdf:about="http://gams.uni-graz.at/thesaurus/mss-conservation">
        <dc:title>Conservation of Medieval Manuscripts</dc:title>
        <skos:hasTopConcept rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
