    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/waterDamage">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Water damage</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Wasserschaden</skos:prefLabel>
        <skos:definition xml:lang="en">Water damage are caused either by floods or the extinguishing water in case of fire. These include stains and discolourations, felting, mould, stuck sheets and warping of the text block or whole book due to excessive moisture.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 117-139 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Salvaging library and archive collections. British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/salvaging-library-and-archive-collections-preservation-guide.pdf</dct:bibliographicCitation>            
        </skos:definition>        
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/fireDamage">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Fire damage</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Brandschaden</skos:prefLabel>
        <skos:definition xml:lang="en">The Archive Damage Atlas classifies fire damage as chemical one, because burning is essentially oxidation (a reaction with oxygen). Possible damages caused by fire, gas, smoke and indirect heat: charred areas; fragile and brittle leaves; illegible writing; leaves burnt along their edges; smudge. In the event of fire books often suffer furthermore damages through the impact of water and extinguishing agents.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 36-41 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Salvaging library and archive collections. British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/salvaging-library-and-archive-collections-preservation-guide.pdf</dct:bibliographicCitation>            
            <dct:bibliographicCitation>Völkel Laura et al. Assessing Fire-Damage in Historical Papers and Alleviating Damage with Soft Cellulose Nanofibers".  In Nano-Micro Small 18 (2022). https://doi.org/10.1002/smll.202105420</dct:bibliographicCitation>
            <dct:bibliographicCitation>Pottier Fabien et al. "Recovering illegible writings in fire-damaged medieval manuscripts through data treatment of UV-fluorescence photography". Journal of Cultural Heritage 36(2019): 183-190. https://hal.science/hal-03027994</dct:bibliographicCitation>
        </skos:definition>        
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/warping">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Warping</skos:prefLabel>
        <skos:altLbel xml:lang="en">Distortion</skos:altLbel>
        <skos:prefLabel xml:lang="de">Verwerfungen</skos:prefLabel>
        <skos:definition xml:lang="en">Deformation by which the cover, often together with the book block, bends outwards on the open side of the book.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/mechanical">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Mechanical damage</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Mechanische Schäden</skos:prefLabel>
        <skos:definition xml:lang="en">Damage due to wear, transport or mishandling the material. May include kinks, folds and cracks of the leaves, which may further lead to tears, fractures or even detached leaves or fragments thereof.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 81-99 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
        </skos:definition>        
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/fold">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Fold</skos:prefLabel>
        <skos:altLbel xml:lang="en">Crease</skos:altLbel>
        <skos:altLbel xml:lang="en">Wrinkle</skos:altLbel>
        <skos:prefLabel xml:lang="de">Falte</skos:prefLabel>
        <skos:altLbel xml:lang="de">Knick</skos:altLbel>
        <skos:definition xml:lang="en">Deformation of leaves which weaken its structure and can further lead to tears.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/cut">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Cut</skos:prefLabel>
        <skos:prefLbel xml:lang="de">Schnitt</skos:prefLbel>
        <skos:definition xml:lang="en">Cuts made by a knife or a pair of scissors usually have sharp edges, which distinguish them from tears.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/hole">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Hole</skos:prefLabel>
        <skos:prefLbel xml:lang="de">Loch</skos:prefLbel>
        <skos:definition xml:lang="en">Some holes were made deliberately in the procedure of making a manuscript, such as piercings for the lines of the text, the margins, but also for ticketing to hold together the quires in the right order before bookbinding as a help for the bookbinders. The preparation for the sewing includes also tiny slits or piercings in the gutter to accommodate the sewing thread.
        For holes made by insects and rodents see the respective entries under 'Pest infestation'.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/loss">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Loss</skos:prefLabel>
        <skos:prefLbel xml:lang="de">Fehlstelle</skos:prefLbel>
        <skos:definition xml:lang="en">Loss is a broader term, which may be applied to any detached or missing parts of the book block or binding.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Book and Paper Group Wiki. 2022. Book and Paper Group Wiki. American Institute for Conservation (AIC). Accessed April 17, 2023. https://www.conservation-wiki.com/wiki/Book_and_Paper_Group_Wiki</dct:bibliographicCitation>
            <dct:bibliographicCitation>Adam Agnes and Wolfgang Kreuzer. Die Fehlstellenergänzung von Pergament am Einband und am Schriftträger. Forschungsblog (accessed 13.04.2023) https://www.onb.ac.at/news-einzelansicht/die-fehlstellenergaenzung-von-pergament-am-einband-und-am-schrifttraeger</dct:bibliographicCitation>
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>  
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/lacuna">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Lacuna</skos:prefLabel>
        <skos:altLbel xml:lang="en">Lacuna</skos:altLbel>
        <skos:definition xml:lang="en">Lacuna is an area of the writing support which is physically detached or missing, be it by having been cut out by people or eaten by rodents or caused by aggressive substances.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/loss"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/tear">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Tear</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Riss</skos:prefLabel>
        <skos:altLbel xml:lang="de">Abrisse</skos:altLbel>
        <skos:altLabel xml:lang="de">Beschädigung durch Zerreißen</skos:altLabel>
        <skos:definition xml:lang="en">Tears may be observed particularly on the leaves' edges (tears along the edges or else known as frayed edges). In most cases these are due to careless use, often in combination with improper storage.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 81-99 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>            
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/detachedLeaves">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Detached leaves and/or gatherings</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Lose Blätter und/oder Lagen</skos:prefLabel>
        <skos:definition xml:lang="en">Loose leaves or gatherings, detached from the book block.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/tornLeaves">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Torn out leaves</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Zerrissene Blätter</skos:prefLabel>
        <skos:definition xml:lang="en">Leaves which are torn out from the book block and missing.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/mechanical"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/gelatinization">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Gelatinization</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Pergamentgelatinierung</skos:prefLabel>
        <skos:definition xml:lang="en">Occurs on parchment and is caused by elevated humidity and temperature. The appearance is a yellow glossy glass-like and hard parchment.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/paperDegradation">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Acidic paper decay</skos:prefLabel>
        <skos:altLabel xml:lang="en">Paper degradation</skos:altLabel>
        <skos:prefLabel xml:lang="de">Papierabbau</skos:prefLabel>
        <skos:definition xml:lang="en">Unlike parchment, paper is frequently prone to degradation, due in part to alkaline agents such as lime and chalk that are used in the manufacturing process.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>    
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/discolouration">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Discolouration</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Ausbleichen</skos:prefLabel>
        <skos:altLbel xml:lang="de">Verbräunen</skos:altLbel>
        <skos:altLbel xml:lang="de">Verfärbung</skos:altLbel>
        <skos:definition xml:lang="en">Colour change can be caused by internal reaction of substances or by light impact. Any dye (including some inks) can fade to lighter or even invisible colour. Green pigments change to brown over time and red and while lead pigments change to black.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/mould">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Mould</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Schimmel</skos:prefLabel>
        <skos:definition xml:lang="en">Mould is a type of fungus. In favourable conditions its spores can quickly germinate and cause severe damage: from small spots and discolourations to decomposition of parts of the paper.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 128-133 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Mould outbreaks in library and archive collections (PDF format). British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/mould-outbreaks-in-libraries-preservation-guide.pdf</dct:bibliographicCitation>
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/pest">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Pest infestation</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Schädlingsbefall</skos:prefLabel>
        <skos:definition xml:lang="en">Pests, such as insects and mice, can cause enormous damage to book and archival materials. Based on the pattern, size and edges of the holes a trained conservator can differentiate between damages caused by rodents (rats and mice) or insects.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value> 
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 101-115 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Managing Pests in Paper-Based Collections. British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/managing-pests-in-paper-based-collections-guide.pdf</dct:bibliographicCitation>
        </skos:definition> 
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/rodents">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Damages by rodents</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Schäden durch Nagetier</skos:prefLabel>
        <skos:definition xml:lang="en">Rats and mice gnaw parts of books to keep their teeth sharp or use shredded paper to make nests. The holes they produce are usually larger. Additionally, mouse urine and droppings can stain the surface and present a disease hazard (cf. British library guide). </skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value> 
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 114-115 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Managing Pests in Paper-Based Collections. British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/managing-pests-in-paper-based-collections-guide.pdf</dct:bibliographicCitation>
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/pest"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/insects">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Damages by insects</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Schäden durch Insekten</skos:prefLabel>
        <skos:definition xml:lang="en">Various types of insects may attack either the writing surface (paper, wood, parchment etc.) or any additional material applied on it (binding agent, mould, organic debris etc.).</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value> 
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 102-113 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
            <dct:bibliographicCitation>Managing Pests in Paper-Based Collections. British Library conservation guide (accessed 13.04.2023). https://www.bl.uk/britishlibrary/~/media/bl/global/conservation/pdf-guides/managing-pests-in-paper-based-collections-guide.pdf</dct:bibliographicCitation>
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/pest"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/previousRepairs">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/conservation"/>
        <skos:prefLabel xml:lang="en">Previous repairs</skos:prefLabel>
        <skos:altLabel xml:lang="en">Previous conservation treatments</skos:altLabel>
        <skos:prefLabel xml:lang="de">Alte Reparaturen</skos:prefLabel>
        <skos:altLabel xml:lang="de">Frühere konservatorische Maßnahme</skos:altLabel>
        <skos:definition xml:lang="en">Implied are all previous measures intended to secure the conservation of the book. Previous repairs may have been carries out already during or immediately after the production of the manuscript (e.g. repairing holes in the parchment by stitching or gluing additional parchment pieces). In modern times one used materials which in time damaged the document (e.g. tape to glue together fragments or chemical solutions to enhance faded writing).</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>             
            <dct:bibliographicCitation>Archives Damage Atlas, pp. 58-63 and 75-79 (accessed 13.04.2023)
                https://www.metamorfoze.nl/sites/default/files/documents/schadeatlas-2010%20engels.pdf</dct:bibliographicCitation>
        </skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/unintentionalDamage"/>
    </skos:Concept>
    
</rdf:RDF>