    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/deletions">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/deletions-tax"></skos:inScheme>
        <skos:prefLabel xml:lang="de">Tilgungen</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Deletions</skos:prefLabel>
        <skos:note xml:lang="en">List of methods for deleting text in mediaeval manuscripts. Divided in groups based on the tools and techniques applied.</skos:note>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/dryDeletions">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/deletions-tax"></skos:inScheme>
        <skos:prefLabel xml:lang="en">Dry methods</skos:prefLabel>        
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/deletions"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/penknife">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/deletions-tax"></skos:inScheme>
        <skos:prefLabel xml:lang="en">Penknife</skos:prefLabel>   
        <skos:altLabel xml:lang="en">knife</skos:altLabel>
        <skos:prefLabel xml:lang="de">Radiermesser</skos:prefLabel>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/dryDeletions"/>
        <skos:exactMatch rdf:resource="http://codicologia.irht.cnrs.fr/term-1554"/>
        <skos:note xml:lang="en">Penknives can be used to delete text in all inks on papyrus, parchment and paper. Traces left: thinner and rougher areas. Detection: lighter area (detected using transmitted light), darker area (detected using raking light); locally, smeared inks; ink written over the area in most cases blurring (this may also be the case with other dry deleting methods, however if full pages are cleaned they are often re-polished after this, which is actually not the case when knife was used).</skos:note>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/pumice">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/deletions-tax"></skos:inScheme>
        <skos:prefLabel xml:lang="en">Pumice</skos:prefLabel>   
        <skos:altLabel xml:lang="en">pumice stone</skos:altLabel>
        <skos:prefLabel xml:lang="de"></skos:prefLabel>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/dryDeletions"/>
        <skos:exactMatch rdf:resource="http://codicologia.irht.cnrs.fr/term-1558"/>
        <skos:note xml:lang="en">Pumice was used to erase text written in all inks on papyrus and parchment. Traces left: parallel scratches visible under magnification and in raking light as darker area. Detectable by: transmitted light (lighter area), raking light (darker/rougher area).</skos:note>
    </skos:Concept>
