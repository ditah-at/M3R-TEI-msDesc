    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:prefLabel xml:lang="de">Schemmränder</skos:prefLabel>
