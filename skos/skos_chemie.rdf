    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/red-ochre">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/chemie-tax"/>
        <skos:prefLabel xml:lang="de">Roter Ocker</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Red ochre</skos:prefLabel>
        <skos:definition xml:lang="en">Natural clay earth pigment, produced from rocks and soils primarily composed of oxides and hydrooxides of iron; hence the main chemical element present is iron (Fe) and oxygen (O). The red ochre contains the mineral hematite. The pigment is prepared by washing, levigation and grindings; most contain impurities (other minerals such as quartz, clays, gypsum, micas, feldspars, etc.). The precise composition and colour depend on the local (geological) specifications. The colour can be further altered by roasting - red becomes purplich or darkened; red colour can be produced also by roasting yellow ochre.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-pigments"/>
        <!--        as skos:related the chemical elements Fe and O rdf resource external skos at: http://data.loterre.fr/ark:/67375/37T - under CC 4.0-->
        <skos:relatedMatch rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-NQ0K580N-5"/>
        <skos:relatedMatch rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-RX5D8P7R-1"/>
<!--        should one add separate SKOS for hierarchical classification of pigments as types? (i.e. pigments -> organic + inorganic => minerals + earth pigments + synthetic 
        and add skos:relatedMatch rdf:resource="..../earth-pigment"-->
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/sinopia">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/chemie-tax"/>
        <skos:prefLabel xml:lang="de"></skos:prefLabel>
        <skos:prefLabel xml:lang="en">Sinopia</skos:prefLabel>
        <skos:definition xml:lang="en">Earth pigment. Vaiety of red quarz; used as a base for the application of gold or in the composition of mixtures of colour; used also in writing, for preparatory drawings (Agati). Main chemical elements: iron (Fe) and oxygen (O).</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-pigments"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/cinnabar">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/chemie-tax"/>
        <skos:prefLabel xml:lang="de">Zinnober </skos:prefLabel>
        <skos:altLabel xml:lang="de">Cinnabarit</skos:altLabel>
        <skos:prefLabel xml:lang="en">Cinnabar</skos:prefLabel>
        <skos:altLabel xml:lang="en">Cinnabarite</skos:altLabel>
        <skos:definition xml:lang="en">Red inorganic pigment of mineral origin containing mercury sulphate (HgS). Main chemical elements: Hg, S.</skos:definition>
