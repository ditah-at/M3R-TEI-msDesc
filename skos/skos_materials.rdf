        <skos:prefLabel xml:lang="en">Minium</skos:prefLabel>
        <skos:altLabel xml:lang="en">Red lead pigment</skos:altLabel>
        <skos:definition xml:lang="en">Red lead is a synthetic inorganic pigment, lead tetroxide (Pb3O4). It was used in medieval manuscripts both alone and with vermilion.  Main chemical elements: lead (Pb) and oxygen (O).</skos:definition>   
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-RX5D8P7R-1"/>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-NRPKN8GG-H"/>
