    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/brown-colorants">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Braune Farbmittel</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Brown colorants</skos:prefLabel>
        <skos:definition xml:lang="en">Brown colours are often produced by mixing red, yellow, blue and black colourants together. There are also natural occurring brown pigments, the most common being brown ochre and umber, which receive their brown colour due to the iron and manganese content.</skos:definition>
        <skos:example>https://upload.wikimedia.org/wikipedia/commons/e/ef/Brown_tints_and_shades.svg</skos:example>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/colorants"/>
