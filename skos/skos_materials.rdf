        <skos:altLabel xml:lang="en">Rubia tinctorum</skos:altLabel>
        <skos:definition xml:lang="en">Red organic pigment made from dried and threshed madder root (Rubia tinctorum). The main chemical elements are: carbon (C), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-RX5D8P7R-1"/>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-GPJ5KG69-K"/>
