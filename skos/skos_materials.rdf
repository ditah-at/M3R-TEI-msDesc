    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/sepia">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Sepia</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Sepia</skos:prefLabel>
        <skos:definition xml:lang="en">Sepia is a brown organic pigment produced from the ink of the cuttlefish (Sepia officinalis). </skos:definition>
