        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-SX8VX34H-4"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/kermes">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Kermes</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Kermes</skos:prefLabel>
        <skos:altLabel xml:lang="en">Carmine</skos:altLabel>
        <skos:altLabel xml:lang="en">Crimson</skos:altLabel>
        <skos:definition xml:lang="en">Red dyestuff made from the larvae of various species of Coccoidea (scale insects) native in the Mediterranean region. The insect cases and larvae were collected together with the twig on which they had clustered, leading sometimes to the false conclusion that the dye derived from the leaves of trees (Eastaugh 2008). The dried insects had to be boiled in water in order to extract the carminic or kermesic acid. The dye was prepared then by precipitation of the extract from previously dyed material.</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Eastaugh, Nicholas, Valentine Walsh, Tracey Chaplin, and Ruth Siddall. Pigment Compendium: A Dictionary and Optical Microscopy of Historical Pigments. Amsterdam: Elsevier, Butterworth-Heinemann, 2008, pp. 216-217.</dct:bibliographicCitation>
            <dct:bibliographicCitation>ColourLex https://colourlex.com/project/madder-lake/</dct:bibliographicCitation>
        </skos:definition>
        <skos:example>https://upload.wikimedia.org/wikipedia/commons/2/21/Kermes_-_Neve_Tzuf.jpg</skos:example>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-RX5D8P7R-1"/>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-GPJ5KG69-K"/>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-DTLN6DML-5"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/kermes-lake">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Kermeslack</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Kermes lake</skos:prefLabel>
        <skos:definition xml:lang="en">A red lack produced by adding alum (aluminium (Al) to the dyestuff made from the larvae of various species of Coccoidea (scale insects) native in the Mediterranean region.</skos:definition>
