<!-- ################################## RED #######################-->
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/red-colorants">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Rote Farbmittel</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Red colorants</skos:prefLabel>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/red-ochre">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Roter Ocker</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Red ochre</skos:prefLabel>
        <skos:definition xml:lang="en">Natural clay earth pigment, produced from rocks and soils primarily composed of oxides and hydroxides of iron; hence the main chemical element present is iron (Fe) and oxygen (O). The red ochre contains the mineral hematite. The pigment is prepared by washing, levigation and grindings; most contain impurities (other minerals such as quartz, clays, gypsum, micas, feldspars, etc.). The precise composition and colour depend on the local (geological) specifications. The colour can be further altered by roasting - red becomes purplich or darkened; red colour can be produced also by roasting yellow ochre.</skos:definition>       
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Iron"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/sinopia">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Sinopia</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Sinopia</skos:prefLabel>
        <skos:definition xml:lang="en">Earth pigment. Variety of red quartz; used as a base for the application of gold or in the composition of mixtures of colour; used also in writing, for preparatory drawings (Agati). Main chemical elements: iron (Fe) and oxygen (O).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Iron"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/cinnabar">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Zinnober</skos:prefLabel>
        <skos:altLabel xml:lang="de">Cinnabarit</skos:altLabel>
        <skos:prefLabel xml:lang="en">Cinnabar</skos:prefLabel>
        <skos:altLabel xml:lang="en">Cinnabarite</skos:altLabel>
        <skos:definition xml:lang="en">Red inorganic pigment of mineral origin containing mercury sulphate (HgS). Main chemical elements: mercury (Hg) and sulfur (S).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Mercury_(element)"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/vermilion">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Synthetischer Zinnober</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Vermilion</skos:prefLabel>
        <skos:definition xml:lang="en">Red inorganic pigment; the synthetic form of cinnabar. Main chemical elements: mercury (Hg) and sulfur (S).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Mercury_(element)"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/minium">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Mennige</skos:prefLabel>
        <skos:altLabel xml:lang="de">Bleimennige</skos:altLabel>
        <skos:prefLabel xml:lang="en">Minium</skos:prefLabel>
        <skos:altLabel xml:lang="en">Red lead pigment</skos:altLabel>
        <skos:definition xml:lang="en">Red lead is a synthetic inorganic pigment, lead tetroxide (Pb3O4). It was used in medieval manuscripts both alone and with vermilion.  Main chemical elements: lead (Pb) and oxygen (O).</skos:definition>   
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Lead"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/madder">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Krapp</skos:prefLabel>
        <skos:altLabel xml:lang="de">Färberkrapp</skos:altLabel>
        <skos:prefLabel xml:lang="en">Madder</skos:prefLabel>
        <skos:altLabel xml:lang="en">Rubia tinctorum</skos:altLabel>
        <skos:definition xml:lang="en">Red organic pigment made from dried and threshed madder root (Rubia tinctorum). The main chemical elements are: carbon (C), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/madder-lake">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Krapplack</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Madder lake</skos:prefLabel>
        <skos:definition xml:lang="en">Red organic pigment made from dried and threshed madder root (Rubia tinctorum), with additive aluminum to prepare a lake. The main chemical elements are: carbon (C), oxygen (O), hydrogen (H) + aluminium (Al).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Aluminium"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/brazilin">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Brasilin</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Brazilin</skos:prefLabel>
        <skos:definition xml:lang="en">Organic pigment made from brazilwood, which was chopped into small pieces and rasped to facilitate oxidation. Almost always used as a lake. Purple-pink in colour; mixed with lead white it gives a bright rose colour, while mixed with woad it gives a purple colour. The main chemical elements are: carbon (C), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/brazilin-lake">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Brasilinlack</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Brazilin lake</skos:prefLabel>
        <skos:definition xml:lang="en">Organic pigment made from brazilwood, which was chopped into small pieces and rasped to facilitate oxidation. Almost always used as a lake. Purple-pink in colour; mixed with lead white it gives a bright rose colour, while mixed with woad it gives a purple colour. The main chemical elements are: carbon (C), oxygen (O), hydrogen (H) + calcium (Ca).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Calcium"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/kermes">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Kermes</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Kermes</skos:prefLabel>
        <skos:altLabel xml:lang="en">Carmine</skos:altLabel>
        <skos:altLabel xml:lang="en">Crimson</skos:altLabel>
        <skos:definition xml:lang="en">Organic pigment made from the larvae of an insect of the cochineal family (the occus ilicis) found in Southern Europe and North Africa. The main chemical elements are: carbon (C), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/kermes-lake">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Kermeslack</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Kermes lake</skos:prefLabel>
        <skos:definition xml:lang="en">Organic pigment made from the larvae of an insect of the cochineal family (the occus ilicis) and used as a lake. The main chemical elements are: carbon (C), oxygen (O), hydrogen (H) + aluminium (Al).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Carbon"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Aluminium"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
        <skos:narrower rdf:resource="http://gams.uni-graz.at/thesaurus/ink-red"/>
    </skos:Concept>
    <!-- #############  YELLOW  #############  -->
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/yellow-colorants">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Gelbe Farbmittel</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Yellow colorants</skos:prefLabel>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/yellow-ochre">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Gelber Ocher</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Yellow ochre</skos:prefLabel>
        <skos:altLabel xml:lang="en">Yellow ocher</skos:altLabel>
        <skos:altLabel xml:lang="en">Gold ochre</skos:altLabel>
        <skos:altLabel xml:lang="en">Limonite</skos:altLabel>
        <skos:definition xml:lang="en">Yellow ochre is produced from rocks and soils primarily composed of oxides and hydrooxides of iron; hence the main chemical element present is iron (Fe). The yellow colour is due to the goethite and/or jarosite group minerals. The pigment is prepared by washing, levigation and grindings; most contain impurities (other minerals such as quartz, clays, gypsum, micas, feldspars, etc.). The precise composition and colour depend on the local (geological) specifications. The colour can be further altered by roasting - yellow becomes often red or brown.</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Iron"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/orpiment">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Auripigment</skos:prefLabel>
        <skos:altLabel xml:lang="de">Arsenblende</skos:altLabel>
        <skos:altLabel xml:lang="de">Gelbe Arsenblende</skos:altLabel>
        <skos:altLabel xml:lang="de">Rauschgelb</skos:altLabel>
        <skos:altLabel xml:lang="de">Operment</skos:altLabel>
        <skos:prefLabel xml:lang="en">Orpiment</skos:prefLabel>
        <skos:definition xml:lang="en">In Latin auripigmentum. Yellow or greenish yellow sulfide of arsenic (arsenic trisulfide: As2S3), hence the main chemical elements are arsenic (As) and sulfur (S). The natural orpiment is an inorganic pigment, found as massive encrusting or granular deposits and as volcanic sublimates (mineral formed from volcanic gas) - particularly from Vesuvius and the Phlegrean Fields in Italy. Other deposits are found by hot springs worldwide. Apart from its natural form, orpiment was also produced synthetically (cf. Cennini, Il libro dell'Arte, who also warns about its poisonous fumes).
            Orpiment reacts with copper- and lead-based pigments (particularly lead white, verdigris and azurite) so artists avoided using these pigments together unless orpiment could be used in isolation (as by heraldic blazonry). Due to its toxicity, orpiment was sometimes used to protect manuscripts against insects.</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Arsenic"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/lead-tin-yellow">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Bleizinngelb</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Lead-tin-yellow</skos:prefLabel>
        <skos:definition xml:lang="en">Lead-tin yellow is a synthetic pigment used mainly for oil painting from the thirteenth to eighteenth centuries. According to its chemical composition there are two type, whereby the first one was more commonly used. Type I is a lead tin oxide (Pb2SnO4), the so-called type II is a silicate (Pb(Sn,Si)O3. The main chemical elements are hence: lead (Pb), tin (Sn) and oxygen (O).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Tin"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Lead"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/weld">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Färber-Wau</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Weld</skos:prefLabel>
        <skos:definition xml:lang="en">Weld is a natural dye made from the plant Reseda luteola. To produce the pigment one has to strew the whole dried plant in a wear solution of alum, precipitating it with the addition of calcium sulfate, eggshell or lead white. The main chemical elements are: aluminium (Al), potassium (K), sulfur (S), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Aluminium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Potassium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/saffron">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Safran</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Saffron</skos:prefLabel>
        <skos:definition xml:lang="en">Natural organic dye mixed with alum. The main chemical elements are: aluminium (Al), potassium (K), sulfur (S), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Aluminium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Potassium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/buckthorn-berries-yellow">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Kreuzdornbeeren (unreif)</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Buckthorn unripe</skos:prefLabel>
        <skos:definition xml:lang="en">The yellow dye is made of various species of the Rhamnaceae family (most common species throughout Europe were the Rhamnus catharticus) To produce yellow dye the berries must be unripe, while ripe ones give a green colour. For producing the pigment they were mixed with alum. The main chemical elements are: aluminium (Al), potassium (K), sulfur (S), oxygen (O) and hydrogen (H).</skos:definition>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Aluminium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Potassium"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Sulfur"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Oxygen"/>
        <skos:relatedMatch rdf:resource="https://en.wikipedia.org/wiki/Hydrogen"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/yellow-colorants"/>
    </skos:Concept>
