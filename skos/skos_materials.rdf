    <!-- #############  BROWN  #############  -->
    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/brown-colorants">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Braune Farbmittel</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Brown colorants</skos:prefLabel>
        <skos:definition xml:lang="en">Brown colours are often produced by mixing red, yellow, blue and black colourant together. There are also natural occuring brown pigments, the most common being brown ochre and umber, which receive their brown colour due to the iron and manganese content.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/colorants"/>
    </skos:Concept>
   ? <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/bister">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Bister</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Bister</skos:prefLabel>
        <skos:definition xml:lang="en">Bister, a carbon-based brown-black pigment, was produced from the soot of burnt wood, tar and resin. Bister was used in manuscript illumination since at least the fourteenth century (Ball 2002).</skos:definition>
        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Kühn, Hermann. Reclams Handbuch der künstlerischen Techniken. Vol. 1. Farbmittel, Buchmalerei, Tafel-und Leinwandmalerei. 3 vols. Stuttgart: Reclam, 1984, p. 41.</dct:bibliographicCitation>
            <dct:bibliographicCitation>Eastaugh, Nicholas, Valentine Walsh, Tracey Chaplin, and Ruth Siddall. Pigment Compendium: A Dictionary and Optical Microscopy of Historical Pigments. Amsterdam: Elsevier, Butterworth-Heinemann, 2008, p. .</dct:bibliographicCitation>
