    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/bronze">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Bronze</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Bronze</skos:prefLabel>
        <skos:definition xml:lang="en">Bronze contains copper (Cu) and other additives such as tin (Sn), silicon, aluminum, manganese, phosphorus, or various other elements. One could hence differentiaate between aluminium bronze (aluminium), speculum metal (tin), etc. </skos:definition>
