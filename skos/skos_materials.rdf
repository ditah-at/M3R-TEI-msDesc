        <skos:definition rdf:parseType="Resource">
            <rdf:value>References</rdf:value>
            <dct:bibliographicCitation>Kühn, Hermann. Reclams Handbuch der künstlerischen Techniken. Vol. 1. Farbmittel, Buchmalerei, Tafel-und Leinwandmalerei. 3 vols. Stuttgart: Reclam, 1984, p. 22.</dct:bibliographicCitation>            
            <dct:bibliographicCitation>Material archiv https://materialarchiv.ch/de/ma:material_887/</dct:bibliographicCitation>
            <dct:bibliographicCitation>ColourLex https://colourlex.com/project/realgar/</dct:bibliographicCitation>
        </skos:definition>
