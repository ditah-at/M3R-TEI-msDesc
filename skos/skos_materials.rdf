    <skos:Concept rdf:about="http://gams.uni-graz.at/thesaurus/organic-brown-colorants">
        <skos:inScheme rdf:resource="http://gams.uni-graz.at/thesaurus/mss-materials"/>
        <skos:prefLabel xml:lang="de">Organische braune Farbmittel</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Organic brown colorants</skos:prefLabel>
        <skos:definition xml:lang="en">Among the organic brown colorants are sepia and bister, which came in use during the late middle ages.</skos:definition>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/brown-colorants"/>
