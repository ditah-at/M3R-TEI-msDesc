        <skos:prefLabel xml:lang="en">Vermilion</skos:prefLabel>
        <skos:definition xml:lang="en">Red inorganic pigment; the synthetic form of cinnabar. Main chemical elements: mercury (Hg) and sulfur (S).</skos:definition>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-NK1JKPQC-4"/>
        <skos:related rdf:resource="http://data.loterre.fr/ark:/67375/8HQ-H9J0KWB5-L"/>
        <skos:broader rdf:resource="http://gams.uni-graz.at/thesaurus/red-colorants"/>
